<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<% Page_LoadComplete(); %>
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title>ASP.NET Page</title>
    <link href="Css/FrameLayout.css" rel="stylesheet" type="text/css" />
    <link href="Css/Topbar.css" rel="stylesheet" type="text/css" />
    <link href="Css/MenuBar.css" rel="stylesheet" type="text/css" />
    <link href="Css/MenuSub.css" rel="stylesheet" type="text/css" />
    <script src='<%=VirtualPathUtility.ToAbsolute("~/Js/DetectBrowser.js")%>' type="text/javascript"></script>
    <script src='<%=VirtualPathUtility.ToAbsolute("~/Js/WebState.js")%>' type="text/javascript"></script>
    <script src='<%=VirtualPathUtility.ToAbsolute("~/Js/Js.js")%>' type="text/javascript"></script>
    <script src='<%=VirtualPathUtility.ToAbsolute("~/Js/Gcoop.js")%>' type="text/javascript"></script>
    <script type="text/javascript">
       
        var isMenuGroupOpen = true;
        var msForm1 = document.forms['<%=form1.ClientID%>'];
        Gcoop.Url = '<%=state.SsUrl%>';
        Gcoop.Application = '<%=state.SsApplication%>';
        Gcoop.CurrentPage = '<%=state.CurrentPage%>';

        function ClickMenuGroup() {
            var divX = document.getElementById('<%=divMenuGroup.ClientID%>'); //currTabElem.setAttribute("className", "some_class_name"); 
            if (isMenuGroupOpen) {
                divX.setAttribute("class", "menubarGroupClose");
                divX.setAttribute("className", "menubarGroupClose");
            } else {
                divX.setAttribute("class", "menubarGroupOpen");
                divX.setAttribute("className", "menubarGroupOpen");
            }
            isMenuGroupOpen = !isMenuGroupOpen;
        }

        function FrameLoadComplete() {
            
            ClickMenuGroup();
            Page_LoadComplete();
        }

        function GoExit() {
            window.location = Gcoop.GetUrl() + "Exit.aspx";
        }

        function GoHome() {
            window.location = Gcoop.GetUrl() + "WebForm1.aspx?exitCommand=Home";
        }

        function GoReport() {
            window.open('<%=VirtualPathUtility.ToAbsolute("~/ReportDefault.aspx")%>');
        }
        function GoChangePass() {
            window.open('<%=VirtualPathUtility.ToAbsolute("~/ChangePass.aspx")%>');
        }
    </script>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
</head>
